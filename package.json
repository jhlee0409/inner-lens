{
  "name": "inner-lens",
  "version": "0.4.1",
  "description": "Self-Debugging QA Agent - Universal bug reporting widget with AI-powered analysis for any frontend framework",
  "author": "jack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhlee0409/inner-lens.git"
  },
  "bugs": {
    "url": "https://github.com/jhlee0409/inner-lens/issues"
  },
  "homepage": "https://github.com/jhlee0409/inner-lens#readme",
  "keywords": [
    "debugging",
    "qa",
    "ai",
    "bug-report",
    "developer-tools",
    "react",
    "vue",
    "vanilla-js",
    "nextjs",
    "vite",
    "remix",
    "astro",
    "github-actions",
    "error-tracking"
  ],
  "type": "module",
  "bin": {
    "inner-lens": "dist/cli.cjs",
    "create-inner-lens": "dist/create.cjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/core.d.ts",
        "default": "./dist/core.js"
      },
      "require": {
        "types": "./dist/core.d.cts",
        "default": "./dist/core.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    },
    "./vanilla": {
      "import": {
        "types": "./dist/vanilla.d.ts",
        "default": "./dist/vanilla.js"
      },
      "require": {
        "types": "./dist/vanilla.d.cts",
        "default": "./dist/vanilla.cjs"
      }
    },
    "./vue": {
      "import": {
        "types": "./dist/vue.d.ts",
        "default": "./dist/vue.js"
      },
      "require": {
        "types": "./dist/vue.d.cts",
        "default": "./dist/vue.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      }
    }
  },
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts",
    "dist/*.d.cts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "sync:check": "npx tsx scripts/sync-check.ts",
    "sparse:check": "npx tsx scripts/sparse-checkout-check.ts",
    "workflow:check": "npx tsx scripts/workflow-isolation-check.ts",
    "prepublishOnly": "npm run build",
    "example": "node examples/vanilla/server.js",
    "example:dev": "npm run build && node examples/vanilla/server.js"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "rrweb": ">=2.0.0-alpha.17",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "rrweb": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@clack/prompts": "^0.11.0",
    "@octokit/app": "^15.0.0",
    "@octokit/rest": "^21.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "@vercel/node": "^2.3.0",
    "@vitest/coverage-v8": "^4.0.16",
    "ai": "^6.0.12",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^27.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rrweb": "^2.0.0-alpha.17",
    "tree-sitter-typescript": "^0.23.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "web-tree-sitter": "^0.26.3",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
