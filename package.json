{
  "name": "inner-lens",
  "version": "1.0.0",
  "description": "Self-Debugging QA Agent - Universal bug reporting widget with AI-powered analysis for any frontend framework",
  "author": "jack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhlee0409/inner-lens.git"
  },
  "keywords": [
    "debugging",
    "qa",
    "ai",
    "bug-report",
    "developer-tools",
    "react",
    "vue",
    "svelte",
    "vanilla-js",
    "nextjs",
    "vite",
    "remix",
    "astro",
    "github-actions",
    "error-tracking"
  ],
  "type": "module",
  "bin": {
    "inner-lens": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/core.d.ts",
        "default": "./dist/core.js"
      },
      "require": {
        "types": "./dist/core.d.cts",
        "default": "./dist/core.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    },
    "./vanilla": {
      "import": {
        "types": "./dist/vanilla.d.ts",
        "default": "./dist/vanilla.js"
      },
      "require": {
        "types": "./dist/vanilla.d.cts",
        "default": "./dist/vanilla.cjs"
      }
    },
    "./vue": {
      "import": {
        "types": "./dist/vue.d.ts",
        "default": "./dist/vue.js"
      },
      "require": {
        "types": "./dist/vue.d.cts",
        "default": "./dist/vue.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      }
    },
    "./replay": {
      "import": {
        "types": "./dist/replay.d.ts",
        "default": "./dist/replay.js"
      },
      "require": {
        "types": "./dist/replay.d.cts",
        "default": "./dist/replay.cjs"
      }
    }
  },
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^10.2.2",
    "rrweb": "^2.0.0-alpha.17",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitest/coverage-v8": "^4.0.16",
    "ai": "^4.0.0",
    "jsdom": "^27.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
